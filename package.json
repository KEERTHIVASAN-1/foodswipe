{
  "name": "foodss",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:front\" \"npm:dev:back\"",
    "dev:front": "cd client && vite --config vite.config.ts",
    "dev:back": "nodemon backend/server.js",
    "build": "cd client && vite build --config vite.config.ts",
    "preview": "cd client && vite preview --config vite.config.ts"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "framer-motion": "11.11.17",
    "three": "0.170.0",
    "gsap": "3.12.5",
    "express": "^4.21.2",
    "mongoose": "^8.7.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "joi": "^17.13.3",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "express-rate-limit": "^7.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "concurrently": "^9.0.0",
    "nodemon": "^3.1.0",
    "mongodb-memory-server": "^10.1.4"
  }
}
